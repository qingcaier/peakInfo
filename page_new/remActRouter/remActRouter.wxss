.routerContain {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 100%;
  /* justify-content: space-around; */
}

.routerContain__item {
  box-sizing: border-box;
  width: 94%;
  height: 180rpx;
  /* padding: 50rpx; */
  background-color: #c89b40;
  color: #ffffff;
  font-size: 36rpx;
  border-radius: 10rpx;
  -webkit-border-radius: 10rpx;
  -moz-border-radius: 10rpx;
  -ms-border-radius: 10rpx;
  -o-border-radius: 10rpx;
  box-shadow: 2rpx 2rpx 10rpx rgba(0, 0, 0, 0.2);
  margin: 30rpx 0;
line-height: 180rpx;
padding: 0 0 0 50rpx;
position: relative
}
.bg-img{
  
    position:absolute;/*此处不用，图片不加载*/
    width: 100%;
    height: 100%;
    z-index: -1
} 
.icon-img{
  height: 160rpx;
position: absolute;
left: 150rpx;
margin: 10rpx 0;


